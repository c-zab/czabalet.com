{
	"name": "czabalet.com",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint"
	},
	"dependencies": {
		"core-js": "^2.6.5",
		"aos": "^2.3.4",
		"vue": "^2.6.10"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^3.9.0",
		"@vue/cli-plugin-eslint": "^3.9.0",
		"@vue/cli-service": "^3.9.0",
		"babel-eslint": "^10.0.1",
		"bootstrap": "^4.3.1",
		"eslint": "^5.16.0",
		"eslint-plugin-vue": "^5.0.0",
		"jquery": "^3.4.1",
		"mixitup": "^3.3.1",
		"popper.js": "^1.14.7",
		"sass": "^1.21.0",
		"sass-loader": "^7.1.0",
		"vue-template-compiler": "^2.6.10"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"rules": {
			"indent": [
				2,
				"tab"
			],
			"no-tabs": "off",
			"comma-dangle": 0,
			"no-console": "off"
		},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	]
}
